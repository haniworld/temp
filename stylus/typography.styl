
/* FILE: typography.styl
 * ---------------------
 * Defines fonts and font stacks to be used throughout the
 * site.
 */




/* FONTS
 * -----
 * Various custom fonts being added to the application
 */

@font-face
	font-family 'DINPro'
	font-weight 300
	src url('/static/fonts/DINPro-Regular_13937.ttf') format('truetype')

@font-face
	font-family 'DINPro'
	font-weight 500
	src url('/static/fonts/DINPro-Medium_13936.ttf') format('truetype')

@font-face
	font-family 'DINPro'
	font-weight 700
	src url('/static/fonts/DINPro-Bold_13934.ttf') format('truetype')
